<?xml version="1.0"?>
<capable-switch xmlns="urn:onf:config:yang">
  <id>openvswitch</id>
  <resources>
    <flow-table xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0" nc:operation="create">
      <resource-id>my_flowtable</resource-id>
      <table-id>123</table-id>
      <name>flowtable</name>
    </flow-table>
  </resources>
  <logical-switches>
    <switch>
      <id>ofc-bridge</id>
      <resources>
        <flow-table>my_flowtable</flow-table>
      </resources>
    </switch>
  </logical-switches>
</capable-switch>

